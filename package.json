{
  "name": "webvtt-language",
  "displayName": "WebVTT Language",
  "description": "Language Support for WebVTT",
  "authors": [
    "Spaceribs (https://keybase.io/spaceribs)",
    "Proudust (https://proudust.github.io/)"
  ],
  "publisher": "spaceribs",
  "repository": {
    "type": "git",
    "url": "https://github.com/spaceribs/vscode-webvtt-language.git"
  },
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "version": "0.0.3",
  "license": "MIT",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "vtt",
        "aliases": [
          "WebVTT",
          "vtt"
        ],
        "extensions": [
          ".vtt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "vtt",
        "scopeName": "source.vtt",
        "path": "./syntaxes/vtt.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "vsce": "^1.81.1"
  }
}
